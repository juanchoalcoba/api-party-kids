{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bookings",
      "dest": "/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",  
        "Access-Control-Allow-Methods": "GET, POST, PUT, PATCH, DELETE",  
        "Access-Control-Allow-Headers": "Content-Type, Authorization",  
        "Access-Control-Allow-Credentials": "true"  
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
